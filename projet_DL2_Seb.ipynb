   "source": [
    "## 3. Restricted Boltzman Machine (RBM)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "def init_RBM(input_size, output_size):\n",
    "    '''\n",
    "    Initialize the parameters of the RBM\n",
    "    '''\n",
    "    a = np.zeros((1, input_size))\n",
    "    b = np.zeros((1, output_size))\n",
    "    W = np.random.normal(0, 0.01, (input_size, output_size))\n",
    "    return a, b, W"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Test\n",
    "RBM = init_RBM(320, 36)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "def entree_sortie_RBM(RBM, V):\n",
    "    '''\n",
    "    Compute the output of the RBM\n",
    "    V : input data, visible, shape (n_samples, input_size)\n",
    "    '''\n",
    "    _, b, W = RBM\n",
    "    return 1/(1 + np.exp(-(np.dot(V, W) + b))) ## shape (n_samples, output_size)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sortie_entree_RBM(RBM, H):\n",
    "    '''\n",
    "    Compute the input of the RBM\n",
    "    H : output data, hidden, shape (n_samples, output_size)\n",
    "    '''\n",
    "    a, _, W = RBM\n",
    "    return 1/(1 + np.exp(-(np.dot(H, W.T) + a))) ## shape (n_samples, input_size)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": []
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "def train_RBM(RBM, input_data, n_epochs = 1, lr=1e-3, batch_size =8, verbose = True):\n",
    "    '''\n",
    "    Train the RBM\n",
    "    '''\n",
    "    a, b, W = RBM\n",
    "    loss = []\n",
    "    n_samples = input_data.shape[0]\n",
    "    for epoch in range(n_epochs):\n",
    "        for i in range(0, n_samples//batch_size * batch_size, batch_size):\n",
    "            V = input_data[i:i+batch_size].reshape(batch_size, -1)\n",
    "            H = entree_sortie_RBM(RBM, V)\n",
    "            V_prime = sortie_entree_RBM(RBM, H)\n",
    "            H_prime = entree_sortie_RBM(RBM, V_prime)\n",
    "            a += lr * (V - V_prime).sum(axis = 0)\n",
    "            b += lr * (H - H_prime).sum(axis = 0)\n",
    "            W += lr * (np.dot(V.T, H) - np.dot(V_prime.T, H_prime))\n",
    "            loss.append(((V - V_prime)**2).mean())\n",
    "        if verbose:\n",
    "            print('Epoch %d/%d' % (epoch+1, n_epochs))\n",
    "            print('Loss : %.4f' % loss[-1])\n",
    "    return a, b, W"
   ]
  },
