  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Retrieving all the labels/characters\n",
    "char_list_all = []\n",
    "for array in data_alphadigits['classlabels']:\n",
    "    char_list_all.append(array[0])\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "def char_to_index(char):\n",
    "    '''\n",
    "    Convert a character to its corresponding index in the dataset\n",
    "    '''\n",
    "    assert char in char_list_all, \"character not in the 36 digits categories\"\n",
    "    return char_list_all.index(char)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "def lire_alpha_digit(data, char_list):\n",
    "    X = []\n",
    "    y = []\n",
    "    char_list = np.unique([char.upper() for char in char_list])\n",
    "    for char in char_list:\n",
    "        for i in range(39):\n",
    "            X.append(data['dat'][char_to_index(char)][i].reshape(1,-1))\n",
    "        y.append(char)\n",
    "    return np.concatenate(X), np.array(y) ## X.shape (39*n_chars, 320), y.shape (39*n_chars,)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
